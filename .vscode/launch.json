    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Catcountants with Samsung A32",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/Catcountants/run_device.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Inspect Catcountants with Samsung A32",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/Catcountants/run_device.py",
            "args": ["-m", "inspector"],
            "console": "integratedTerminal"
        },
    ]
}